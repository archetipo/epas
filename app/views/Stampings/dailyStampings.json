{
  "surnarName": "${personDay.person.surname}",
  "name": "${personDay.person.name}",
  "day": "${personDay.date}",
  "progressive": "${personDay.progressive}",
  "mealTicket": "${personDay.mealTicket()}",
  "absence": "${personDay.absence != null ? personDay.absence.absenceCode : null}",
  "stampings" : {
  %{ 
  	for (stamping in personDay.getStampings()) {
  		%}
  			{
  			"date" : "${stamping.date}",
  			"wayType" : ${stamping.way},
  			"isServiceExit" : ${stamping.isServiceExit},
  			"stampModificationType : "${stamping.stampModificationType != null ? stamping.stampModificationType.code : ''}"
  			"note" : "${stamping.note}"
  			},		
  		%{
  	}
  }% 
  }
}
