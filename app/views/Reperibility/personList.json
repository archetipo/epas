[
  %{ 
  	for (person in personList) {
  		%}
  			{
  			"id" : "${person.id}",
  			"email" : "${person.contactData.email}",
  			"name" : "${person.name}",
  			"surname" : "${person.surname}",
  			"mobile": "${person.contactData.mobile}"
  			}${person_isLast ? '' : ','}		
  		%{
  	}
  }% 
]
